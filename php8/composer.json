{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "phpro/grumphp": "~v1.14",
        "phpstan/extension-installer": "~1.2",
        "phpstan/phpstan": "~1.9",
        "phpstan/phpstan-doctrine": "~1.3",
        "phpstan/phpstan-symfony": "~1.2",
        "squizlabs/php_codesniffer": "~3.0"
    },
    "config": {
        "vendor-dir": "/opt/vendor",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "phpro/grumphp": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "/code/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "/code/tests/"
        }
    }
}
